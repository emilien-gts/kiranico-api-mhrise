        "name": "@string@"
      },
      "description": "@string@",
      "objective": "@string@",
      "failureConditions": "@string@",
      "monsters": "@string@",
      "items": "@string@"
    },
    "@...@"
  ],
  "hydra:view": {
    "@id": "/api/quests?page=1",
    "@type": "hydra:PartialCollectionView",
