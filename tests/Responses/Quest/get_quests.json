  "@type": "hydra:Collection",
  "hydra:totalItems": "@integer@",
  "hydra:member": [
    {
      "@id": "@string@.contains('/api/quests/')",
      "@type": "Quest",
      "id": "@uuid@",
      "type": "event||anomaly||follower_collab||hub_master||hub_high||hub_low||village||arena||training",
      "name": "@string@",
      "client": {
        "@id": "@string@",
        "@type": "Client",
