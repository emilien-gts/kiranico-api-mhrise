{
  "@context": "/api/contexts/Item",
  "@id": "/api/items",
  "@type": "hydra:Collection",
  "hydra:totalItems": "@integer@",
  "hydra:member": [
    {
      "@id": "@string@",
      "@type": "Item",
      "id": "@string@",
      "type": "consume||material||scrap||ammo||account||antique",
      "name": "@string@",
      "description": "@string@",
      "imageUrl": "@string@.isUrl()"
    },
    "@...@"
  ],
  "hydra:view": {
    "@id": "/api/items?page=1",
    "@type": "hydra:PartialCollectionView",
    "hydra:first": "/api/items?page=1",
    "hydra:last": "@string@",
    "hydra:next": "@string@"
  }
  }